{% extends "theme::layouts/error" %}

{% block content %}

<div class="uk-container uk-container-small uk-flex-auto uk-text-center" data-uk-scrollspy="target: > .animate; cls: uk-animation-slide-bottom-small uk-invisible; delay: 300">
  <h1 class="uk-heading-primary animate uk-invisible" style="font-weight: 700;">Oops!</h1>

  <div class="uk-width-1-1 uk-width-3-5@s  uk-margin-auto animate uk-invisible">
    <span class="uk-label">404 ERROR</span>
    <p class="lead">The page you are looking for is as far as the East is from the West. Yeah, we tried but we can't find it.</p>
    <hr class="uk-divider-small">
    <p class="uk-text-center uk-text-meta uk-light">Head back to <a href="{{ config_get("app.url") }}">{{ config_get("app.name") }}</p>
  </div>
 
</div>

{% endblock %}
